<view class="content data-v-37154e3e"><view class="fiex data-v-37154e3e"><view data-event-opts="{{[['tap',[['change',[1]]]]]}}" class="{{['border data-v-37154e3e',(type==1)?'active':'']}}" bindtap="__e"><text class="data-v-37154e3e">交易中（元）</text></view><view data-event-opts="{{[['tap',[['change',[2]]]]]}}" class="{{['data-v-37154e3e',(type==2)?'active':'']}}" bindtap="__e"><text class="data-v-37154e3e">结算中（元）</text></view></view><block wx:if="{{dataList.length>0}}"><view class="boxs data-v-37154e3e"><view class="top data-v-37154e3e"><view class="money data-v-37154e3e">{{(type==1?'交易中订单金额':'结算中订单金额')+' :'}}<text class="color data-v-37154e3e">{{"￥"+(type==1?money||0:money1||0)+"元"}}</text></view></view><view class="list data-v-37154e3e"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item_list data-v-37154e3e"><view class="data-v-37154e3e"><text class="data-v-37154e3e">{{"订单支付时间:"+item.createTime}}</text><text class="r data-v-37154e3e">结算中</text></view><view class="box data-v-37154e3e"><view class="data-v-37154e3e"><image src="{{item.originalImg||item.pictUrl||srcArr[item.source]}}" class="data-v-37154e3e"></image></view><view class="left data-v-37154e3e"><view class="title data-v-37154e3e">{{item.productName||item.itemTitle||""}}</view><view class="data-v-37154e3e"><text class="price data-v-37154e3e">{{"￥"+(item.payMoney||item.alipayTotalPrice||"")}}</text><text class="r data-v-37154e3e">{{"x"+(item.productNum||item.itemNum||"")}}</text></view></view></view></view></block><uni-load-more vue-id="1" status="{{status}}" class="data-v-37154e3e" bind:__l="__l"></uni-load-more></view></view></block><block wx:else><view class="no data-v-37154e3e"><image class="noData data-v-37154e3e" src="../../static/noData.png"></image><view class="data-v-37154e3e">暂无数据</view></view></block></view>