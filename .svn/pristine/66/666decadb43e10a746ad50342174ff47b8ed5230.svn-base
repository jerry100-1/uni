<view class="content data-v-5151ce8e"><view class="input-row border data-v-5151ce8e"><text class="title data-v-5151ce8e">取现密码</text><input type="password" placeholder="6-16位数字、字母、符号" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e" class="data-v-5151ce8e"/></view><view class="input-row border data-v-5151ce8e"><text class="title data-v-5151ce8e">确认密码</text><input type="password" placeholder="请再次取现密码 " data-event-opts="{{[['input',[['__set_model',['','passwordok','$event',[]]]]]]}}" value="{{passwordok}}" bindinput="__e" class="data-v-5151ce8e"/></view><view class="input-row border data-v-5151ce8e"><text class="title data-v-5151ce8e">手机号码</text><text class="phone data-v-5151ce8e">{{phone}}</text></view><view class="input-row data-v-5151ce8e"><text class="title data-v-5151ce8e">验证码</text><input type="text" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e" class="data-v-5151ce8e"/><view class="getCode data-v-5151ce8e" type="primary" data-event-opts="{{[['tap',[['sendCode',['$event']]]]]}}" bindtap="__e">{{codeSend?time+"s":"获取验证码"}}</view></view><view class="data-v-5151ce8e"><button type="primary" data-event-opts="{{[['tap',[['sava',['$event']]]]]}}" bindtap="__e" class="data-v-5151ce8e">确认</button></view></view>