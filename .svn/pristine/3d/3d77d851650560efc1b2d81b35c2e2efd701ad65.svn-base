<view class="content data-v-f0cd8ff8"><block wx:if="{{salesList.length>0}}"><view class="data-v-f0cd8ff8"><view class="top data-v-f0cd8ff8"><view class="money data-v-f0cd8ff8">销售总金额:<text class="color data-v-f0cd8ff8">{{"￥"+money+"元"}}</text></view></view><view class="list data-v-f0cd8ff8"><block wx:for="{{salesList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item_list data-v-f0cd8ff8"><view class="data-v-f0cd8ff8"><text class="data-v-f0cd8ff8">{{"订单完成时间:"+item.confirmTime}}</text><text class="r data-v-f0cd8ff8">交易成功</text></view><view class="box data-v-f0cd8ff8"><view class="data-v-f0cd8ff8"><block wx:if="{{item.payType==1}}"><image src="{{item.originalImg||''}}" class="data-v-f0cd8ff8"></image></block><block wx:else><image src="../../static/pro/down.png" class="data-v-f0cd8ff8"></image></block></view><view class="left data-v-f0cd8ff8"><view class="title data-v-f0cd8ff8">{{item.productName||""}}</view><view class="data-v-f0cd8ff8"><text class="price data-v-f0cd8ff8">{{"￥"+(item.payMoney||"")}}</text><text class="r data-v-f0cd8ff8">{{"x"+(item.productNum||"")}}</text></view></view></view></view></block><uni-load-more vue-id="1" status="{{status}}" class="data-v-f0cd8ff8" bind:__l="__l"></uni-load-more></view></view></block><block wx:else><view class="no data-v-f0cd8ff8"><image class="noData data-v-f0cd8ff8" src="../../static/noData.png"></image><view class="data-v-f0cd8ff8">暂无数据</view></view></block></view>