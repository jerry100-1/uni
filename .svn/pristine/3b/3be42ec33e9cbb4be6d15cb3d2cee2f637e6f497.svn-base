<view class="content data-v-c31b6560"><view class="msgType data-v-c31b6560"><view class="list data-v-c31b6560"><view class="data-v-c31b6560"><view class="item_list data-v-c31b6560" data-type="3" data-event-opts="{{[['tap',[['msgTypeChange',['$event']]]]]}}" bindtap="__e"><image src="../../static/user/message_icon3.png" class="data-v-c31b6560"></image><text class="{{['data-v-c31b6560',(msgType==3)?'active':'']}}">系统消息</text></view><view class="item_list data-v-c31b6560" data-type="4" data-event-opts="{{[['tap',[['msgTypeChange',['$event']]]]]}}" bindtap="__e"><image src="../../static/user/message_icon4.png" class="data-v-c31b6560"></image><text class="{{['data-v-c31b6560',(msgType==4)?'active':'']}}">入账通知</text></view></view></view><view class="whiteBg data-v-c31b6560"></view></view><view class="box data-v-c31b6560"><block wx:if="{{msgList&&msgList.length>0}}"><view class="list data-v-c31b6560"><block wx:for="{{msgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item data-v-c31b6560"><view class="title data-v-c31b6560">{{item.createTime}}</view><view class="item_list data-v-c31b6560"><view class="item_con data-v-c31b6560"><view class="data-v-c31b6560"><image class="proImg data-v-c31b6560" src="{{item.resources}}"></image></view><view class="msg_item data-v-c31b6560"><view class="msgText data-v-c31b6560">{{item.title}}</view><block wx:if="{{item.val}}"><view class="data-v-c31b6560">{{'交易时间:'+item.val.updateTime}}</view></block><block wx:if="{{item.val}}"><view class="msgtitle data-v-c31b6560">{{"交易商品:"+item.val.productNum}}</view></block><block wx:if="{{item.val}}"><view class="data-v-c31b6560">{{'交易金额:￥'+item.val.payMoney}}</view></block><block wx:if="{{item.val}}"><view class="data-v-c31b6560">{{'订单状态:'+orderTypeArr[item.val.orderType]}}</view></block><block wx:if="{{item.val}}"><view class="data-v-c31b6560">{{'商品价格:￥'+item.val.productPrice}}</view></block><view class="data-v-c31b6560">{{'附加消息:'+item.comment}}</view></view></view><block wx:if="{{msgType==2&&item.orderType==3}}"><view data-event-opts="{{[['tap',[['order',[3]]]]]}}" class="footer data-v-c31b6560" bindtap="__e"><text class="data-v-c31b6560">订单发货</text><image src="../../static/reight.png" class="data-v-c31b6560"></image></view></block><block wx:if="{{msgType==2&&item.orderType==7}}"><view data-event-opts="{{[['tap',[['order',[-1]]]]]}}" class="footer data-v-c31b6560" bindtap="__e"><text class="data-v-c31b6560">立即退款</text><image src="../../static/reight.png" class="data-v-c31b6560"></image></view></block></view></view></block><uni-load-more vue-id="1" status="{{status}}" class="data-v-c31b6560" bind:__l="__l"></uni-load-more></view></block><block wx:else><view class="no data-v-c31b6560"><image class="noData data-v-c31b6560" src="../../static/noData.png"></image><view class="data-v-c31b6560">暂无数据</view></view></block></view></view>